{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "@{DefaultConnection}"
  },
  "GraphApiCredentialOptionsForClient": {
    "TenantId": "@{PublicPortalTenantId}",
    "ClientId": "@{PublicPortalGraphApiAppClientId}",
    "GraphApiClientSecret": "@{PublicPortalGraphApiAppClientSecret}",
    "Domain": "@{PublicPortalDomain}"
  },
  "GraphApiCredentialOptionsForAdmin": {
    "TenantId": "@{DepartmentPortalTenantId}",
    "ClientId": "@{DepartmentPortalGraphApiAppClientId}",
    "GraphApiClientSecret": "@{DepartmentPortalGraphApiAppClientSecret}",
    "Domain": "@{DepartmentPortalDomain}"
  },
  "GlobalAppSettings": {
    "OtpExpirationTime": 2,
    "MaxOtpIncorrectCount": 3,
    "ResetPasswordTokenExpiryHours": 24,
    "LoginTokenExpiryHours": 24,
    "JwtSecretKey": "@{JwtSecretKey}"
  },
  "ApplicationInsights": {
    "ConnectionString": "@{ApplicationInsightsConnectionString}"
  }
}